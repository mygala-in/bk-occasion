{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "max-len": "off",   
        "no-multiple-empty-lines": "off",
        "object-curly-newline": "off",
        "import/no-relative-packages": "off",
        "import/extensions": [
            "error",
            "ignorePackages",
            {
            "js": "always"
            }
        ],
        "import/prefer-default-export": "off"
    }
}
